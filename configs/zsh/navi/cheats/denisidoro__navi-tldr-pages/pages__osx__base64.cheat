% base64, osx

# Encode a file
base64 -i <plain_file>

# Decode a file
base64 -D -i <base64_file>

# Encode from `stdin`
echo -n <plain_text> | base64

# Decode from `stdin`
echo -n <base64_text> | base64 -D
